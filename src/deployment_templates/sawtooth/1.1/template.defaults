# note this is an example values file and should not be actually used

affinity:
  enabled: false

imagePullSecrets:
  enabled: true
  value: 
    - regcred
sawtooth:
  permissioned: false
  namespace: default
  networkName: sawtooth
  scheduler: parallel
  devmode:
    enabled: true
  pbft:
    enabled: false 
  raft:
    enabled: false
  poet:
    enabled: false
    targetWaitTime: 30
    initialWaitTime: 25
    maxBatchesPerBlock: 100
    enclaveModule: simulator
  genesis:
    enabled: true
  dynamicPeering: false
  externalSeeds: []
  xo:
    enabled: false
  smallbank:
    enabled: false
  customTPs:
          #    - name: intkey-tp
          #      image: "blockchaintp/sawtooth-intkey-tp-go:1.0.5"
          #      command: [ "bash", "-c" ]
          #      args: [ "intkey-tp-go -v --connect tcp://localhost:4004" ]
    
images:
  grafana: dev.catenasys.com:8083/blockchaintp/sawtooth-stats-grafana:latest
  influxdb: dev.catenasys.com:8083/blockchaintp/sawtooth-stats-influxdb:latest
  validator: dev.catenasys.com:8083/blockchaintp/sawtooth-validator:latest
