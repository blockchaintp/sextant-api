{
  "name": "sextant-api",
  "private": true,
  "version": "1.0.0",
  "description": "api server for the sextant cluster creation service",
  "main": "index.js",
  "scripts": {
    "migrate": "knex migrate:latest",
    "download-helm-charts": "node scripts/download-helm-charts.js",
    "copy-helm-fixtures": "node -e 'require(\"./test/fixtures/helmFixtures\").copyFixtures()'",
    "remove-helm-fixtures": "node -e 'require(\"./test/fixtures/helmFixtures\").removeFixtures()'",
    "preserve": "npm run download-helm-charts && npm run migrate",
    "serve": "node src/index.js",
    "generate-swagger": "node src/generateSwagger.js",
    "pretest": "npm run copy-helm-fixtures",
    "test": "npm run cover:test && npm run cover:report && npm run test:report",
    "posttest": "npm run remove-helm-fixtures",
    "test:tape": "tape {src,test}/**/*.test.js > /tmp/test.out",
    "test:report": "cat /tmp/test.out |tap-spec && cat /tmp/test.out | tap-junit > /tmp/junit.xml",
    "cover:test": "nyc --silent npm run test:tape",
    "cover:report": "nyc report --report-dir /tmp --reporter=text --reporter=lcov",
    "start": "npm run download-helm-charts && npm run serve"
  },
  "dependencies": {
    "async": "^3.2.0",
    "aws-regions": "^2.2.0",
    "aws-sdk": "2.928.0",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "connect-pg-simple": "^6.2.1",
    "cookie-parser": "^1.4.5",
    "deepmerge": "^4.2.2",
    "dotenv": "^10.0.0",
    "dotty": "^0.1.1",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "express-session": "^1.17.2",
    "fs-extra": "^10.0.0",
    "get-port": "^5.1.1",
    "http-proxy": "^1.18.1",
    "joi": "^13.6.0",
    "js-yaml": "^3.14.1",
    "jsonist": "^2.1.2",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.6",
    "minimist": "^1.2.5",
    "mkdirp": "^0.5.5",
    "node-cidr": "^1.0.0",
    "openapi-types": "^9.0.3",
    "passport": "^0.4.1",
    "pg": "7.18.2",
    "pino": "^4.17.6",
    "randomstring": "^1.2.1",
    "request": "^2.88.2",
    "sawtooth-sdk": "^1.1.1",
    "split": "^1.0.1",
    "swagger-jsdoc": "^5.0.1",
    "swagger-ui-express": "^4.1.6",
    "tmp": "^0.2.1",
    "widdershins": "^4.0.1",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "eslint": "^7.28.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.23.4",
    "nyc": "^15.1.0",
    "tap-junit": "^5.0.0",
    "tap-spec": "^5.0.0",
    "tape": "^4.13.3"
  },
  "resolutions": {
    "lodash": "~4.17.13"
  }
}
