{
  "name": "sextant-api",
  "private": true,
  "version": "1.0.0",
  "description": "api server for the sextant cluster creation service",
  "main": "index.js",
  "scripts": {
    "migrate": "knex migrate:latest",
    "download-helm-charts": "node scripts/download-helm-charts.js",
    "preserve": "npm run download-helm-charts && npm run migrate",
    "serve": "node src/index.js",
    "pretest": "npm run download-helm-charts",
    "test": "tape {src,test}/**/*.test.js | tap-spec",
    "start": "npm run download-helm-charts && npm run serve"
  },
  "dependencies": {
    "@digitalasset/daml-ledger": "0.8.3",
    "async": "^2.6.0",
    "aws-regions": "^1.4.1",
    "aws-sdk": "2.548.0",
    "axios": "^0.19.0",
    "bcrypt": "^3.0.2",
    "bluebird": "^3.5.4",
    "body-parser": "^1.17.2",
    "connect-pg-simple": "^6.0.1",
    "cookie-parser": "^1.4.3",
    "deepmerge": "^3.2.0",
    "dotenv": "^8.2.0",
    "dotty": "^0.1.0",
    "express": "^4.15.3",
    "express-async-handler": "^1.1.4",
    "express-session": "^1.15.6",
    "fs-extra": "^9.0.1",
    "get-port": "^5.0.0",
    "http-proxy": "^1.18.1",
    "joi": "^13.6.0",
    "js-yaml": "^3.13.1",
    "jsonist": "^2.1.0",
    "jsonwebtoken": "^8.5.0",
    "knex": "^0.21.1",
    "minimist": "^1.2.5",
    "mkdirp": "^0.5.1",
    "node-cidr": "^1.0.0",
    "passport": "^0.4.0",
    "pg": "7.17.1",
    "pino": "^4.17.6",
    "randomstring": "^1.1.5",
    "request": "^2.81.0",
    "sawtooth-sdk": "^1.0.5",
    "split": "^1.0.1",
    "tap-spec": "^5.0.0",
    "tmp": "^0.0.33",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "tape": "^4.9.1"
  },
  "resolutions": {
    "lodash": "~4.17.13"
  }
