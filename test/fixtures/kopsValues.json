{
    "kops": {
        "stateStore": "s3://clusters.dev.catenasys.com"
    },
    "cloud": {
        "provider": "aws"
    },
    "cluster": {
        "name": "apples",
        "domainName": "dev.catenasys.com",
        "master": {
            "machineType": "m4.large",
            "zones": [
                {
                    "subnet": "eu-west-2a",
                    "minSize": 1,
                    "maxSize": 1
                },
                {
                    "subnet": "eu-west-2b",
                    "minSize": 1,
                    "maxSize": 1
                },
                {
                    "subnet": "eu-west-2c",
                    "minSize": 1,
                    "maxSize": 1
                }
            ]
        },
        "node": {
            "machineType": "m4.large",
            "zones": [
                {
                    "subnet": "eu-west-2a",
                    "minSize": 2,
                    "maxSize": 2
                },
                {
                    "subnet": "eu-west-2b",
                    "minSize": 1,
                    "maxSize": 1
                }
            ]
        }
    },
    "topology": {
        "dns": {
            "type": "Public"
        },
        "masters": "public",
        "nodes": "public"
    },
    "networkCIDR": "m4.large",
    "subnets": [
        {
            "name": "eu-west-2a",
            "cidr": "172.20.0.0/19"
        },
        {
            "name": "eu-west-2b",
            "cidr": "172.20.32.0/19"
        },
        {
            "name": "eu-west-2c",
            "cidr": "172.20.64.0/19"
        }
    ]
}
